/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp ESC    &kp Q    &kp W   &kp E   &kp R
                &kp TAB    &kp A    &kp S   &kp D   &kp F
                &kp LSHFT  &kp Z    &kp X   &kp C   &kp V
                &kp LCTRL  &kp 1    &kp 2   &kp 3   &kp 4
                &kp SPACE  &kp 5    &kp 6   &kp 7   &kp 8
                &kp ENTER  &kp 9    &kp 0   &kp BSPC &kp DEL
            >;
        };
    };
};
